{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://*:3874",
      "sslPort": 44393
    }
  },
  "profiles": {
     "pokedexApi": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "api/graphql",
      "applicationUrl": "https://*:7094;http://*:5100",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/graphql",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
    "Docker": {
      "commandName": "DockerCompose",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/graphql",
      "publishAllPorts": true,
      "useSSL": true
    }
    }
  }
}
